{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 5
        // "sourceType": "module"
    },
    "extends": "airbnb",
    // "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
        // "prettier"
    ],
    "env": {
        "jquery": true
    },
    "rules": {
        "indent": ["error", 4],
        // "prettier/prettier": "error"
        "no-undef": 1,
        "no-unused-expressions": 1
    }
}
